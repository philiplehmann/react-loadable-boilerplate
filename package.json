{
  "name": "react-loadable-boilerplate",
  "version": "1.0.0",
  "description": "A bleeding-edge React boilerplate that offers code-splitting and server-side rendering",
  "main": "server/server.production.js",
  "repository": "https://github.com/mikebars/react-loadable-boilerplate.git",
  "author": "Mike Bars",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/polyfill": "^7.0.0",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "mkdirp": "^1.0.4",
    "polished": "^3.6.3",
    "raf": "^3.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.6",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.1.0",
    "file-loader": "^6.0.0",
    "flow-bin": "^0.125.1",
    "flow-typed": "^3.1.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.5.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.1"
  },
  "resolutions": {
    "serialize-javascript": ">=3.1.0",
    "braces": ">=2.3.1"
  },
  "scripts": {
    "--TOOLS--": "",
    "test": "yarn run jest",
    "jest": "jest --config tools/jest/config.jest.js --no-cache",
    "lint": "yarn run eslint && yarn run stylelint && yarn run prettier && yarn run flow",
    "eslint": "eslint --config tools/eslint/config.eslint.js ./",
    "flow": "flow check tools/flow",
    "flow-typed:install": "flow-typed update-cache && flow-typed install --libdefDir flow-typed --overwrite --rootDir tools/flow --skip",
    "flow-typed:create-patch": "flow-typed update-cache && flow-typed create-stub --libdefDir flow-typed/patches --overwrite --rootDir tools/flow",
    "flow-typed:create-stub": "flow-typed update-cache && flow-typed create-stub --libdefDir flow-typed/stubs --overwrite --rootDir tools/flow",
    "prettier": "prettier --config tools/prettier/config.prettier.js --write \"./**/*.js\"",
    "stylelint": "stylelint --config tools/stylelint/config.stylelint.js \"**/*.js\"",
    "--DEVELOPMENT--": "",
    "development": "yarn run webpack:development && yarn run node:development",
    "node:development": "node --inspect server/server.development.js",
    "prewebpack:development": "yarn run rimraf:development && yarn run mkdirp:development",
    "mkdirp:development": "mkdirp client/development && mkdirp server/development",
    "rimraf:development": "rimraf client/development && rimraf server/development",
    "webpack:development": "yarn run webpack:client:development && yarn run webpack:server:development",
    "webpack:client:development": "webpack --config tools/webpack/client.development.config.webpack.js --profile --json > client/development/client.development.stats.webpack.json",
    "webpack:server:development": "webpack --config tools/webpack/server.development.config.webpack.js --profile --json > server/development/server.development.stats.webpack.json",
    "--PRODUCTION--": "",
    "production": "yarn run webpack:production && yarn run node:production",
    "node:production": "node server/server.production.js",
    "prewebpack:production": "yarn run rimraf:production && yarn run mkdirp:production",
    "mkdirp:production": "mkdirp client/production && mkdirp server/production",
    "rimraf:production": "rimraf client/production && rimraf server/production",
    "webpack:production": "yarn run webpack:client:production && yarn run webpack:server:production",
    "webpack:client:production": "webpack --config tools/webpack/client.production.config.webpack.js --profile --json > client/production/client.production.stats.webpack.json",
    "webpack:server:production": "webpack --config tools/webpack/server.production.config.webpack.js --profile --json > server/production/server.production.stats.webpack.json"
  }
}
